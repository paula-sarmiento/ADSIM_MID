<?xml version="1.0" encoding="utf-8"?><!-- -*- coding: utf-8; mode: SGML -*- -->

<container n="Discharge_velocity_transient_container" pn="Discharge velocity (transient)" icon="icon_transient" help="Configure and apply transient discharge velocity boundary conditions">
  
  <!-- Configuration settings -->
  <container n="transient_bc_settings" pn="Transient BC Settings" icon="icon_settings" help="Configure settings for transient boundary conditions">
    <value n="num_discharge_time_sets" pn="Number of discharge time sets" v="3" values="2,3,4,5,6,7,8,9,10" help="Specify how many time-discharge pairs will be available"/>
  </container>
  
  <!-- Boundary condition application -->
  <condition n="Transient_Discharge_velocity" pn="Apply discharge velocity" icon="icon_select" ov="point,line,surface" help="Apply time-dependent liquid discharge velocity using piecewise-linear interpolation between defined time points.">
    
    <!-- Time point 1 -->
  <value n="time_1" pn="Time 1 [T]" v="0.0" state="[ADSIM_2025::CheckDischargeTimeSet 1]" help="Time for first point"/>
  <value n="discharge_1" pn="Discharge 1 [L/T]" v="0.0" state="[ADSIM_2025::CheckDischargeTimeSet 1]" help="Discharge velocity at time 1"/>
  
  <!-- Time point 2 -->
  <value n="time_2" pn="Time 2 [T]" v="0.0" state="[ADSIM_2025::CheckDischargeTimeSet 2]" help="Time for second point"/>
